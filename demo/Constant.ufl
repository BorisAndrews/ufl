# Test form for scalar and vector constants.

cell = triangle
element = FiniteElement("Lagrange", cell, 1)

v = TestFunction(element)
u = TrialFunction(element)
f = Function(element)

c = Constant(cell)
d = VectorConstant(cell)

a = c*dot(grad(v), grad(u))*dx
L = inner(d, grad(v))*dx

